  PROBLEM STATEMENT 1:
  Given an array of elements,find the sum.

CODE:

#include<bits/stdc++.h>  // header file which includes all the standard libraries
using namespace std;

int main()
{
  int a[5] = {1,2,4,6,0};
  int sum = 0;

  for(int i = 0;i < 5;i++)
  {
      cout << sum + a[i];
  }

  cout << " Sum of the elements of Array : " << sum << endl;
  return 0;
}

